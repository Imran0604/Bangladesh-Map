# Bangladesh-Map
Bangladesh map create by c language

#include<stdio.h>
#include<stdlib.h>
#include<windows.h>

int main()
{
	for (int i = 1; i <= 27; i++)
	{
		for (int j = 1; j <= 25; j++)
		{
			if (i == 2 && j > 2 && j < 4 || i == 3 && j > 2 && j < 5 || i == 3 && j > 5 && j < 7 || i == 4 && j > 2 && j < 9
			        || i == 5 && j > 1 && j < 11 || i == 6 && j > 2 && j < 10 || i == 7 && j > 4 && j < 10 || i == 8 && j > 6 && j < 12
			        || i == 8 && j > 14 && j < 22 || i == 9 && j > 2 && j < 23 || i == 10 && j > 2 && j < 23
			        || i == 11 && j > 2 && j < 22 || i == 12 && j > 3 && j < 20 || i == 13 && j > 5 && j < 19 || i == 14 && j > 4 && j < 18
			        || i == 14 && j > 20 && j < 23 || i == 15 && j > 4 && j < 17 || i == 15 && j > 4 && j < 18 || i == 15 && j > 20 && j < 23
			        || i == 16 && j > 4 && j < 17 || i == 16 && j > 16 && j < 18 || i == 16 && j > 19 && j < 24 || i == 17 && j > 4 && j < 24
			        || i == 18 && j > 5 && j < 24 || i == 19 && j > 4 && j < 24 || i == 20 && j > 5 && j < 24 || i == 21 && j > 5 && j < 16
			        || i == 21 && j > 19 && j < 24 || i == 22 && j > 6 && j < 11 || i == 22 && j > 19 && j < 25
			        || i == 23 && j > 20 && j < 23 || i == 23 && j > 20 && j < 25 || i == 24 && j > 20 && j < 25
			        || i == 25 && j > 20 && j < 25 || i == 26 && j > 20 && j < 24 || i == 27 && j > 21 && j < 24)
			{
				Sleep(10);
				printf(" *");
			}

			else
			{
				Sleep(10);
				printf("  ");
			}
		}
		printf("\n");
	}
}
